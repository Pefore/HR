@model Model.human_file_digModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>query_list_information</title>
    <link href="~/Content/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script type="text/javascript">
        $(function () {
            $("#sc").click(function () {
                if (window.confirm('要恢复此档案吗')) {
                    var id = $("#id").val();
                    window.location.href = "/human_file_dig/HFZT?id=" + id;
                }
            })
        })
    </script>
</head>
<body>
    @using (Html.BeginForm())
    {
        <form name="humanfileForm" method="post" action="/hr/humanfile.do">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案恢复 </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <span id="sc" class="BUTTON_STYLE1">恢复</span>
                        @Html.ActionLink("返回", "recovery_list", "", new { @class = "BUTTON_STYLE1" });
                    </td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=3 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1" width="10%">
                        档案编号
                    </td>
                    <td colspan="6" class="TD_STYLE2">
                        @Html.EditorFor(model => model.Id, new { htmlAttributes = new { @class = "INPUT_STYLE2", @hidden = "hidden", @id = "id" } })
                        @Html.Label("human_id", Model.human_id)
                    </td>
                    <td rowspan="6" width="13%" style="text-align: center;"></td>
                </tr>
                <tr>
                    <td class="TD_STYLE1" width="10%">
                        I级机构
                    </td>
                    <td width="13%" class="TD_STYLE2">
                        @Html.Label("first_kind_name", Model.first_kind_name)
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        II级机构
                    </td>
                    <td width="13%" class="TD_STYLE2">
                        @Html.Label("second_kind_name", Model.second_kind_name)
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        III级机构
                    </td>
                    <td class="TD_STYLE2" colspan="2" width="2%">
                        @Html.Label("third_kind_name", Model.third_kind_name)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        职位分类
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_major_kind_name ", Model.human_major_kind_name)
                    </td>
                    <td class="TD_STYLE1">
                        职位名称
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("hunma_major_name ", Model.hunma_major_name)
                    </td>
                    <td class="TD_STYLE1">
                        职称
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        @Html.Label("human_pro_designation ", Model.human_pro_designation)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        姓名
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_name", Model.human_name)
                    </td>
                    <td class="TD_STYLE1">
                        性别
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_sex", Model.human_sex)
                    </td>
                    <td class="TD_STYLE1">
                        EMAIL
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        @Html.Label("human_email", Model.human_email)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        电话
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_telephone", Model.human_telephone)
                    </td>
                    <td class="TD_STYLE1">
                        QQ
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_qq", Model.human_qq)
                    </td>
                    <td class="TD_STYLE1">
                        手机
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        @Html.Label("human_mobilephone ", Model.human_mobilephone)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        住址
                    </td>
                    <td colspan="3" class="TD_STYLE2">
                        @Html.Label("human_address  ", Model.human_address)
                    </td>
                    <td class="TD_STYLE1">
                        邮编
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        @Html.Label("human_postcode  ", Model.human_postcode)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        国籍
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_nationality", Model.human_nationality)
                    </td>
                    <td class="TD_STYLE1">
                        出生地
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_birthplace", Model.human_birthplace)
                    </td>
                    <td class="TD_STYLE1">
                        生日
                    </td>
                    <td width="13%" class="TD_STYLE2">
                        @Html.Label("human_birthday", Model.human_birthday.ToString())
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        民族
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_race", Model.human_race)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        宗教信仰
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_religion", Model.human_religion)
                    </td>
                    <td class="TD_STYLE1">
                        政治面貌
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_party", Model.human_party)
                    </td>
                    <td class="TD_STYLE1">
                        身份证号码
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_id_card", Model.human_id_card)
                    </td>
                    <td class="TD_STYLE1">
                        社会保障号码
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_society_security_id", Model.human_society_security_id)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        年龄
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_age ", Model.human_age.ToString())
                    </td>
                    <td class="TD_STYLE1">
                        学历
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_educated_degree ", Model.human_educated_degree)
                    </td>
                    <td class="TD_STYLE1">
                        教育年限
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_educated_years ", Model.human_educated_years.ToString())
                    </td>
                    <td class="TD_STYLE1">
                        学历专业
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_educated_major", Model.human_educated_major)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        薪酬标准
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("salary_standard_name", Model.salary_standard_name)
                    </td>
                    <td class="TD_STYLE1">
                        开户行
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_bank ", Model.human_bank)
                    </td>
                    <td class="TD_STYLE1">
                        帐号
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_account ", Model.human_account)
                    </td>
                    <td class="TD_STYLE1">
                        复核人
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("checker", Model.checker)
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        复核时间
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("check_time", Model.check_time.ToString())
                    </td>
                    <td class="TD_STYLE1">
                        特长
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_speciality", Model.human_speciality)
                    </td>
                    <td class="TD_STYLE1">
                        爱好
                    </td>
                    <td class="TD_STYLE2">
                        @Html.Label("human_hobby", Model.human_hobby)
                    </td>
                    <td class="TD_STYLE1">
                        &nbsp;
                    </td>
                    <td class="TD_STYLE2">
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        档案附件
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <a href="#"></a>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        个人履历
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        @Html.TextAreaFor(model => model.human_histroy_records, new { @class = "TEXTAREA_STYLE1", @Rows = "4", @readonly = "readonly" })
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        家庭关系信息
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        @Html.TextAreaFor(model => model.human_family_membership, new { @class = "TEXTAREA_STYLE1", @Rows = "4", @readonly = "readonly" })
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        备注
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        @Html.TextAreaFor(model => model.remark, new { @class = "TEXTAREA_STYLE1", @Rows = "4", @readonly = "readonly" })
                    </td>
                </tr>
            </table>
        </form>
    }
</body>
</html>
